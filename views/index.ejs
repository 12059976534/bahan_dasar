<% layout('./base') -%>

    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <div class="container">
                    <div class="row mt-5">
                        <%for(var i=0;i< data.length; i++) {%>
                            <div class="col">
                                <a href="/details/<%= data[i] .id%>">
                                    <div class="card" style="width: 18rem;">
                                        <img src="http://127.0.0.1:3001\<%= data[i] .poto%>" class="card-img-top" alt="...">
                                        <div class="card-body">
                                            <p class="card-text"><%= data[i] .namawisata%></p>
                                        </div>
                                    </div>
                                </a>
                            </div>
                            <%}%>
                    </div>
                </div>
            </div>
            <div class="col-md-4"></div>
        </div>
    </div>